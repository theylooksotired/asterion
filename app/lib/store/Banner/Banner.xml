<?xml version="1.0" encoding="utf-8"?>
<object>
	<name>Banner</name>
	<info>
		<sql>
			<primary>idBanner</primary>
			<created>true</created>
			<modified>true</modified>
			<order>true</order>
			<engine>MyISAM</engine>
		</sql>
		<form>
			<title>banners</title>
			<label>#title</label>
			<pager></pager>
			<group></group>
			<orderBy>ord</orderBy>
			<permissions>
				<superadmin>
					<canRewrite>true</canRewrite>
					<canViewList>true</canViewList>
					<canInsert>true</canInsert>
					<canModify>true</canModify>
					<canDelete>true</canDelete>
					<canOrder>false</canOrder>
					<canView>false</canView>
				</superadmin>
			</permissions>
			<labelAdmin>
				<label style="image" type="imageLink">image</label>
				<label style="big">
					<label type="link">_#title</label>
					<label style="small">#description</label>
				</label>
			</labelAdmin>
		</form>
	</info>
	<attributes>
		<attribute>
			<name>idBanner</name>
			<type>id-autoincrement</type>
		</attribute>
		<attribute>
			<name>image</name>
			<label>image</label>
			<type>file</type>
			<mode>image</mode>
		</attribute>
		<attribute>
			<name>title</name>
			<label>title</label>
			<type>text-long</type>
			<required>notEmpty</required>
		</attribute>
		<attribute>
			<name>titleUrl</name>
			<type>hidden-url</type>
			<refAttribute>title</refAttribute>
		</attribute>
		<attribute>
			<name>description</name>
			<label>description</label>
			<type>textarea-large</type>
		</attribute>
		<attribute>
			<name>link</name>
			<label>link</label>
			<type>text-long</type>
		</attribute>
	</attributes>
</object>