<?xml version="1.0" encoding="utf-8"?>
<object>
  <name>User</name>
  <info>
    <sql>
      <primary>idUser</primary>
      <created>true</created>
      <modified>true</modified>
      <order>false</order>
      <engine>MyISAM</engine>
      <onDelete></onDelete>
      <onCreate></onCreate>
    </sql>
    <form>
      <title>users</title>
      <label>#name</label>
      <layout>simple</layout>
      <pager></pager>
      <group></group>
      <orderBy>name</orderBy>
      <label>#name</label>
      <permissions>
        <superadmin>
          <canRewrite>true</canRewrite>
          <canViewList>true</canViewList>
          <canInsert>true</canInsert>
          <canModify>true</canModify>
          <canDelete>true</canDelete>
          <canOrder>true</canOrder>
          <canView>true</canView>
        </superadmin>
      </permissions>
      <labelAdmin>
        <label style="active" type="active">#active</label>
        <label style="regular">
          <label type="link">#name</label>
          <label style="small">#email</label>
        </label>
      </labelAdmin>
    </form>
  </info>
  <attributes>
    <attribute>
      <name>idUser</name>
      <type>id-autoincrement</type>
    </attribute>
    <attribute>
      <name>name</name>
      <label>name</label>
      <required>notEmpty</required>
      <type>text</type>
    </attribute>
    <attribute>
      <name>email</name>
      <label>email</label>
      <required>email</required>
      <type>text-email</type>
    </attribute>
    <attribute>
      <name>password</name>
      <label>password</label>
      <type>password</type>
    </attribute>
    <attribute>
      <name>passwordTemp</name>
      <type>hidden</type>
    </attribute>
    <attribute>
      <name>idUserType</name>
      <label>userType</label>
      <type>select</type>
      <refObject>UserType</refObject>
    </attribute>
    <attribute>
      <name>active</name>
      <label>active</label>
      <type>checkbox</type>
    </attribute>
  </attributes>
  <indexes>
    <index>
      <name>email</name>
      <type></type>
      <fields>email</fields>
    </index>
    <index>
      <name>idUserType</name>
      <type></type>
      <fields>idUserType</fields>
    </index>
  </indexes>
</object>