<?xml version="1.0" encoding="utf-8"?>
<object>
    <name>Params</name>
    <info>
        <sql>
            <primary>idParams</primary>
        </sql>
        <form>
            <title>params</title>
            <orderBy>idParams</orderBy>
            <label>#information</label>
            <permissions>
                <superadmin>
                    <canRewrite>true</canRewrite>
                    <canViewList>true</canViewList>
                    <canInsert>true</canInsert>
                    <canModify>true</canModify>
                    <canDelete>true</canDelete>
                </superadmin>
            </permissions>
            <labelAdmin>
                <label type="link" style="medium">#code</label>
                <label style="regularSmall">#information_#informationExtra</label>
            </labelAdmin>
        </form>
    </info>
    <attributes>
        <attribute>
            <name>idParams</name>
            <type>id-autoincrement</type>
        </attribute>
        <attribute>
            <label>code</label>
            <name>code</name>
            <type>text-unchangeable</type>
            <required>notEmpty</required>
        </attribute>
        <attribute>
            <label>information</label>
            <name>information</name>
            <type>text-long</type>
            <required></required>
        </attribute>
        <attribute>
            <label>informationExtra</label>
            <name>informationExtra</name>
            <type>textarea</type>
            <required></required>
        </attribute>
    </attributes>
    <indexes>
        <index>
            <name>code</name>
            <type></type>
            <fields>code</fields>
        </index>
    </indexes>
</object>