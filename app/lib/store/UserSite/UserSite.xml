<?xml version="1.0" encoding="utf-8"?>
<object>
	<name>UserSite</name>
	<info>
		<sql>
			<primary>idUserSite</primary>
			<created>true</created>
			<modified>true</modified>
			<order>false</order>
			<engine>MyISAM</engine>
		</sql>
		<form>
			<title>users</title>
			<label>#name</label>
			<pager>50</pager>
			<orderBy>name</orderBy>
			<permissions>
				<superadmin>
					<canRewrite>true</canRewrite>
					<canViewList>true</canViewList>
					<canInsert>true</canInsert>
					<canModify>true</canModify>
					<canDelete>true</canDelete>
					<canOrder>false</canOrder>
					<canView>false</canView>
				</superadmin>
			</permissions>
			<labelAdmin>
				<label type="active" style="active">#active</label>
				<label style="long">
					<label type="link">#name</label>
					<label style="small">#email</label>
				</label>
			</labelAdmin>
		</form>
	</info>
	<attributes>
		<attribute>
			<name>idUserSite</name>
			<type>id-autoincrement</type>
		</attribute>
		<attribute>
			<name>email</name>
			<label>email</label>
			<type>text-long</type>
			<required>email</required>
		</attribute>
		<attribute>
			<name>password</name>
			<label>password</label>
			<type>password</type>
			<required>notEmpty</required>
		</attribute>
		<attribute>
			<name>passwordTemp</name>
			<type>hidden</type>
		</attribute>
		<attribute>
			<name>name</name>
			<label>namePerson</label>
			<type>text-long</type>
			<required>notEmpty</required>
		</attribute>
		<attribute>
			<name>lastName</name>
			<label>lastName</label>
			<type>text-long</type>
			<required>notEmpty</required>
		</attribute>
		<attribute>
			<name>birthDate</name>
			<label>birthDate</label>
			<type>date</type>
		</attribute>
		<attribute>
			<name>telephone</name>
			<label>telephone</label>
			<type>text-long</type>
		</attribute>
		<attribute>
			<name>mobile</name>
			<label>mobile</label>
			<type>text-long</type>
		</attribute>
		<attribute>
			<name>address</name>
			<label>address</label>
			<type>text-long</type>
		</attribute>
		<attribute>
			<name>addressAdd</name>
			<label>addressAdd</label>
			<type>text-long</type>
		</attribute>
		<attribute>
			<name>city</name>
			<label>city</label>
			<type>text</type>
		</attribute>
		<attribute>
			<name>country</name>
			<label>country</label>
			<type>text</type>
		</attribute>
		<attribute>
			<name>active</name>
			<label>active</label>
			<type>checkbox</type>
		</attribute>
	</attributes>
	<indexes>
		<index>
			<name>email</name>
			<type></type>
			<fields>email</fields>
		</index>
	</indexes>
</object>