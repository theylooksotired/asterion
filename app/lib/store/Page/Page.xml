<?xml version="1.0" encoding="utf-8"?>
<object>
	<name>Page</name>
	<info>
		<sql>
			<primary>idPage</primary>
			<created>false</created>
			<modified>false</modified>
			<order>false</order>
			<engine>MyISAM</engine>
		</sql>
		<form>
			<title>pages</title>
			<label>#title</label>
			<orderBy>titleUrl</orderBy>
			<permissions>
				<superadmin>
					<canRewrite>true</canRewrite>
					<canViewList>true</canViewList>
					<canInsert>true</canInsert>
					<canModify>true</canModify>
					<canDelete>true</canDelete>
					<canOrder>false</canOrder>
					<canView>false</canView>
				</superadmin>
			</permissions>
			<labelAdmin>
				<label style="long">
					<label type="link">_#title</label>
					<label type="small">_#code</label>
					<label style="small">#metaDescription</label>
				</label>
			</labelAdmin>
		</form>
	</info>
	<attributes>
		<attribute>
			<name>idPage</name>
			<type>id-autoincrement</type>
		</attribute>
		<attribute>
			<name>code</name>
			<label>code</label>
			<type>text-unchangeable</type>
			<required>notEmpty</required>
		</attribute>
		<attribute>
			<name>title</name>
			<lang>true</lang>
			<label>title</label>
			<type>text-long</type>
			<required>notEmpty</required>
		</attribute>
		<attribute>
			<name>titleUrl</name>
			<lang>true</lang>
			<type>hidden-url</type>
			<refAttribute>title</refAttribute>
		</attribute>
		<attribute>
			<name>metaDescription</name>
			<lang>true</lang>
			<label>metaDescription</label>
			<type>textarea</type>
		</attribute>
		<attribute>
			<name>description</name>
			<lang>true</lang>
			<label>description</label>
			<type>textarea-ck</type>
			<required>notEmpty</required>
		</attribute>
	</attributes>
	<indexes>
		<index>
			<name>titleUrl</name>
			<lang>true</lang>
			<type></type>
			<fields>titleUrl</fields>
		</index>
	</indexes>
</object>