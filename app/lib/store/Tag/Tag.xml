<?xml version="1.0" encoding="utf-8"?>
<object>
	<name>Tag</name>
	<info>
		<sql>
			<primary>idTag</primary>
			<engine>MyISAM</engine>
		</sql>
		<form>
			<title>Tag</title>
			<label>#name</label>
			<layout>simple</layout>
			<pager>100</pager>
			<group></group>
			<orderBy>name</orderBy>
			<permissions>
				<superadmin>
					<canRewrite>true</canRewrite>
					<canViewList>true</canViewList>
					<canInsert>true</canInsert>
					<canModify>true</canModify>
					<canDelete>true</canDelete>
				</superadmin>
			</permissions>
		</form>
	</info>
	<attributes>
		<attribute>
			<name>idTag</name>
			<type>id-autoincrement</type>
		</attribute>
		<attribute>
			<name>name</name>
			<label>name</label>
			<type>text-long</type>
		</attribute>
		<attribute>
			<name>nameUrl</name>
			<type>hidden-url</type>
			<refAttribute>name</refAttribute>
		</attribute>
	</attributes>
	<indexes>
		<index>
			<name>nameUrl</name>
			<type></type>
			<fields>nameUrl</fields>
		</index>
	</indexes>
