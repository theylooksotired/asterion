<object>
    <name>UserTypeMenu</name>
    <info>
        <sql>
            <primary>idUserTypeMenu</primary>
            <order>true</order>
        </sql>
        <form>
            <title>menu</title>
            <orderBy>ord</orderBy>
            <group>idUserType</group>
            <label>#name</label>
        </form>
        <nav>
            <labelSections>
                <label type="link">name</label>
                <label>action</label>
            </labelSections>
        </nav>
    </info>
    <attributes>
        <attribute>
            <name>idUserTypeMenu</name>
            <type>id-autoincrement</type>
        </attribute>
        <attribute>
            <name>idUserType</name>
            <type>select</type>
            <refObject>UserType</refObject>
        </attribute>
        <attribute>
            <name>name</name>
            <label>name</label>
            <lang>true</lang>
            <required>notEmpty</required>
            <type>text-large</type>
        </attribute>
        <attribute>
            <name>action</name>
            <label>action</label>
            <required>notEmpty</required>
            <type>text-large</type>
        </attribute>
        <attribute>
            <name>type</name>
            <label>type</label>
            <type>select</type>
            <values>
                <value>typeMenu1</value>
                <value>typeMenu2</value>
                <value>typeMenu3</value>
                <value>typeMenu4</value>
            </values>
        </attribute>
    </attributes>
    <indexes>
        <index>
            <name>idUserType</name>
            <fields>idUserType</fields>
        </index>
    </indexes>
</object>