<?xml version="1.0" encoding="utf-8"?>
<object>
	<name>PostPicture</name>
	<info>
		<sql>
			<primary>idPostPicture</primary>
			<created>false</created>
			<modified>false</modified>
			<order>true</order>
			<engine>MyISAM</engine>
			<onDelete></onDelete>
			<onCreate></onCreate>
		</sql>
		<form>
			<title>Sample Object Picture</title>
			<label>#name#</label>
			<layout>simple</layout>
			<pager></pager>
			<group></group>
			<orderBy></orderBy>
			<permissions>
				<superadmin>
					<canRewrite>true</canRewrite>
					<canViewList>true</canViewList>
					<canInsert>true</canInsert>
					<canModify>true</canModify>
					<canDelete>true</canDelete>
					<canOrder>true</canOrder>
					<canView>true</canView>
				</superadmin>
			</permissions>
			<labelAdmin>
				<label type="link">#text#</label>
			</labelAdmin>
		</form>
	</info>
	<attributes>
		<attribute>
			<name>idPostPicture</name>
			<type>id-autoincrement</type>
		</attribute>
		<attribute>
			<name>idPost</name>
			<type>linkid-autoincrement</type>
		</attribute>
		<attribute>
			<name>image</name>
			<label>image</label>
			<type>file</type>
			<mode>image</mode>
		</attribute>
		<attribute>
			<name>name</name>
			<label>name</label>
			<type>text</type>
		</attribute>
		<attribute>
			<name>nameUrl</name>
			<type>hidden-url</type>
			<refAttribute>name</refAttribute>
		</attribute>
		<attribute>
			<name>description</name>
			<label>description</label>
			<type>textarea-small</type>
		</attribute>
	</attributes>
	<indexes>
		<index>
			<name>nameUrl</name>
			<type></type>
			<fields>nameUrl</fields>
		</index>
	</indexes>
</object>