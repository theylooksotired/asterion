<object>
    <name>Params</name>
    <info>
        <sql>
            <primary>idParams</primary>
        </sql>
        <form>
            <title>params</title>
            <orderBy>code</orderBy>
            <label>#information</label>
            <labelAdmin>
                <label type="link" style="medium">#code</label>
                <label style="regularSmall">#information_#informationExtra</label>
            </labelAdmin>
        </form>
    </info>
    <attributes>
        <attribute>
            <name>idParams</name>
            <type>id-autoincrement</type>
        </attribute>
        <attribute>
            <label>code</label>
            <name>code</name>
            <type>text-unchangeable</type>
            <required>notEmpty</required>
        </attribute>
        <attribute>
            <label>information</label>
            <name>information</name>
            <type>textarea</type>
            <required></required>
        </attribute>
    </attributes>
    <indexes>
        <index>
            <name>code</name>
            <type></type>
            <fields>code</fields>
        </index>
    </indexes>
</object>